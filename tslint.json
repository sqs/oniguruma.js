{
    "extends": [
        "tslint:latest"
    ],
    "rules": {
        "eofline": false,
        "array-type": [
            true,
            "array"
        ],
        "arrow-parens": false,
        "switch-default": false,
        "indent": [
            true,
            "tabs"
        ],
        "interface-name": [
            false
        ],
        "max-classes-per-file": [
            false
        ],
        "max-line-length": [
            false
        ],
        "member-access": false,
        "member-ordering": [
            false
        ],
        "no-console": [
            false
        ],
        "no-namespace": false,
        "object-literal-sort-keys": false,
        "ordered-imports": [
            false
        ],
        "quotemark": [
            "single"
        ],
        "variable-name": [
            true,
            "check-format",
            "allow-leading-underscore",
            "allow-pascal-case",
            "ban-keywords"
        ]
    }
}