{
    "version": "0.1.0",
    "command": "yarn",
    "isShellCommand": true,
    "args": [
        "run",
        "build:asmjs"
    ],
    "showOutput": "always",
    "problemMatcher": {
        "owner": "cpp",
        "fileLocation": [
            "relative",
            "${workspaceRoot}/build/asmjs/build"
        ],
        "pattern": {
            "regexp": "^(.*):(\\d+):(\\d+):\\s+(warning|error):\\s+(.*)$",
            "file": 1,
            "line": 2,
            "column": 3,
            "severity": 4,
            "message": 5
        }
    }
}